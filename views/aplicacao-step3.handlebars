<script>
    // Evento de clique no botão "Adicionar"
    const arrayNationalities = []

    document.getElementById('addNationality').addEventListener('click', (e) => {
        e.preventDefault
        const selectNationality = document.getElementById('nationality')
        const selectedOption = selectNationality.options[selectNationality.selectedIndex]
        
        if (selectedOption) {
            const inputNationalities = document.getElementById('nationalitiesExtra')
            const nacionalidadeSelecionada = selectedOption.value

            arrayNationalities.push(selectedOption.value)

            inputNationalities.value = arrayNationalities.toString()

            // Adicionar a nacionalidade selecionada à lista de nacionalidades
            const nacionalidadesSelecionadasDiv = document.getElementById('nationalities')
            const novaNacionalidade = document.createElement('div')
            novaNacionalidade.className = "d-flex flex-row align-items-center w-50 mb-2"
            novaNacionalidade.innerHTML = `
                <button class="removerNacionalidade btn btn-danger me-3"><i class="bi bi-x-circle-fill"></i></button>
                <p class="m-0 p-0">${nacionalidadeSelecionada}</p>
            `

            nacionalidadesSelecionadasDiv.appendChild(novaNacionalidade)

            // Evento de clique no botão "Remover" da nova nacionalidade
            novaNacionalidade.querySelector('.removerNacionalidade').addEventListener('click', () => {
                nacionalidadesSelecionadasDiv.removeChild(novaNacionalidade)
                const indice = arrayNationalities.indexOf(nacionalidadeSelecionada)

                arrayNationalities.splice(indice, 1)               
                
                inputNationalities.value = arrayNationalities.toString()
            })

            // Limpar a seleção no <select>
            selectNationality.selectedIndex = 0
        }
    })
</script>