    function validPresentDay(element) {
        // Remove alerta anterior, se existir
        const existingAlert = document.getElementById(`alertDate${element.id}`)
        if (existingAlert) {
            existingAlert.parentNode.removeChild(existingAlert)
            const envForm = document.getElementById('envForm')
            envForm.disabled = false
        }

        let dateVerificated = new Date(document.getElementById(element.id).value).getTime()
        let presentDay = Date.now()

        if (dateVerificated < presentDay) {
            let siblingElement = document.createElement("div")
            let alertElement = document.getElementById(`alertDate${element.id}`).style.display = 'none'
            siblingElement.innerHTML = ``

            element.parentNode.insertBefore(siblingElement, element.nextSibling)
            const envForm = document.getElementById('envForm')
            envForm.disabled = false
        } else {
            let siblingElement = document.createElement("div")
            siblingElement.id = `alertDate${element.id}`
            siblingElement.innerHTML = `
                <div class="alert alert-danger w-25 my-3">
                    <p class="m-0">Insirá uma data antecessora ao dia de hoje.</p>
                </div>
            `

            element.parentNode.insertBefore(siblingElement, element.nextSibling)
            const envForm = document.getElementById('envForm')
            envForm.disabled = true
        }
    }

